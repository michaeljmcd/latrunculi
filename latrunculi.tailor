[DEFAULT]
verbose = True
projects = darcs-to-svn

[darcs-to-svn]
root-directory = /tmp
source = darcs:latrunculi
target = svn:latrunculi
state-file = darcs-to-svn.state

[svn-to-darcs]
root-directory = /tmp
source = svn:latrunculi
target = darcs:latrunculi
state-file = svn-to-darcs.state
    
[svn:latrunculi]
repository = svn.sourceforge.net
module = /svnroot/latrunculi
subdir = svnside

[darcs:latrunculi]
repository = /home/beowulf/latrunculi
subdir = darcside
